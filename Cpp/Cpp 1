#include <iostream>
using namespace std;

int main() 
   {int age;
    cout << "Entrez votre age : ";
    cin >> age;

    if (age < 18) 
       {cout << "Vous etes mineur." << endl;}
    else if (age <= 60) 
        {cout << "Vous etes adulte." << endl;} 
    else
       {cout << "Vous etes senior." << endl;}
    return 0;}
